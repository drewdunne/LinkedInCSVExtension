{
    "name": "LinkedIn CSV Formatter",
    "description": "Exports LinkedIn Page Data to CSV",
    "version": "0.1.0",
    "manifest_version": 3,
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "tabs"
    ],
    "icons": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png",
      "48": "images/icon-48.png",
      "128": "images/icon-128.png"
    },
    "action": {
      "default_popup": "popup.htm"
    },
  "background": {
    "service_worker": "bundle.js"
 },
    "host_permissions": [
        "https://www.linkedin.com/*"
    ]
}